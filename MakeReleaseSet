#!/bin/sh
rm -rf Dist
if [ $# -ge 1 ]; then
	for arch in $*; do
		echo $arch
		BUILD_DIST=y ARCH=$arch make clean install
	done
else
	BUILD_DIST=y make clean install
fi
tar -zcf Acess2_latest_bin.tar.gz Dist/*
mdeltree b:/Acess2
mcopy -s Dist/ b:/Acess2
gzip -c AcessRelease.img > Acess2_latest.img.gz
